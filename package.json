		"karma-jasmine": "^5.0.1",
		"karma-mocha-reporter": "^2.2.5",
		"karma-rollup-preprocessor": "^7.0.8",
		"minimist": "^1.2.6",
		"nightwatch": "2.0.10",
		"react": "^18.1.0",
		"react-dom": "^18.1.0",
		"rollup": "^2.75.6",
		"rollup-plugin-cleanup": "^3.2.1",
		"rollup-plugin-polyfill-node": "^0.9.0",
		"rollup-plugin-progress": "^1.1.2",
		"rollup-plugin-terser": "^7.0.2",
		"semver": "^7.3.7",
		"shelljs": "^0.8.5",
