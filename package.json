		"karma-webpack": "^4.0.2",
		"mocha": "^8.2.1",
		"react": "^16.5.2",
		"react-dom": "^16.5.2",
		"react-router-dom": "^5.0.0",
		"shelljs": "^0.8.3",
		"sinon": "^9.2.3",
