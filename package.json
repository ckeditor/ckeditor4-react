{
	"name": "ckeditor4-react",
	"version": "1.4.0",
	"description": "Official React component for CKEditor 4 â€“ the best browser-based rich text editor.",
	"license": "(GPL-2.0-or-later OR LGPL-2.1 OR MPL-1.1)",
	"main": "dist/index.cjs.js",
	"module": "dist/index.esm.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist/"
	],
	"scripts": {
		"build": "rollup -c && npm run types:emit",
		"bump": "node ./scripts/bump.js",
		"develop": "karma start",
		"lint": "eslint --ext .js,.ts,.tsx src tests scripts",
		"postversion": "git rm -r --cached dist/ && git commit -m \"Clean after release [ci skip]\" && git push origin && git push origin --tags",
		"preversion": "npm test",
		"start": "rollup -c -w",
		"test:all:e2e": "node ./scripts/e2e-runner.js --react all",
		"test:all:units": "node ./scripts/units-runner.js --react all",
		"test:browser": "karma start --single-run",
		"test": "npm run lint && npm run types:check && npm run test:browser",
		"types:emit": "tsc src/index.ts --jsx react --emitDeclarationOnly --declaration --declarationDir dist",
		"types:check": "tsc --noEmit",
		"version": "npm run build && git add -f dist/"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ckeditor/ckeditor4-react.git"
	},
	"keywords": [
		"wysiwyg",
		"rich text",
		"editor",
		"html",
		"contentEditable",
		"editing",
		"react",
		"react-component",
		"ckeditor",
		"ckeditor4",
		"ckeditor 4"
	],
	"author": "CKSource (http://cksource.com/)",
	"bugs": {
		"url": "https://github.com/ckeditor/ckeditor4-react/issues"
	},
	"homepage": "https://github.com/ckeditor/ckeditor4-react#readme",
	"peerDependencies": {
		"ckeditor4": "^4.16.0",
		"react": "^16.8.3 || ^17"
	},
	"devDependencies": {
		"@babel/core": "^7.13.15",
		"@babel/preset-env": "^7.13.15",
		"@babel/preset-react": "^7.13.13",
		"@babel/preset-typescript": "^7.13.0",
		"@rollup/plugin-babel": "^5.3.0",
		"@rollup/plugin-commonjs": "^18.0.0",
		"@rollup/plugin-node-resolve": "^11.2.1",
		"@rollup/plugin-replace": "^2.4.2",
		"@rollup/plugin-typescript": "^8.2.1",
		"@testing-library/jasmine-dom": "^1.2.0",
		"@testing-library/react": "^11.2.6",
		"@testing-library/react-hooks": "^5.1.1",
		"@types/jasmine": "^3.6.9",
		"@types/react": "^17.0.3",
		"@typescript-eslint/eslint-plugin": "^4.22.0",
		"@typescript-eslint/parser": "^4.22.0",
		"browserstack-local": "^1.4.8",
		"ckeditor4": "^4.16.0",
		"eslint": "^7.21.0",
		"eslint-config-ckeditor5": "^3.1.0",
		"eslint-plugin-react": "^7.22.0",
		"jasmine": "^3.7.0",
		"karma": "^6.3.2",
		"karma-browserstack-launcher": "^1.6.0",
		"karma-chrome-launcher": "^3.1.0",
		"karma-jasmine": "^4.0.1",
		"karma-mocha-reporter": "^2.2.5",
		"karma-rollup-preprocessor": "^7.0.7",
		"minimist": "^1.2.5",
		"nightwatch": "^1.6.3",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-router-dom": "^5.2.0",
		"rollup": "^2.45.2",
		"rollup-plugin-cleanup": "^3.2.1",
		"rollup-plugin-polyfill-node": "^0.6.2",
		"rollup-plugin-progress": "^1.1.2",
		"rollup-plugin-terser": "^7.0.2",
		"semver": "^7.3.4",
		"shelljs": "^0.8.3",
		"tslib": "^2.2.0",
