		"babel-loader": "^8.2.2",
		"chai": "^4.2.0",
		"ckeditor4": "^4.16.0",
		"core-js": "^3.8.2",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.5",
		"eslint": "^7.18.0",
		"eslint-config-ckeditor5": "^3.1.0",
